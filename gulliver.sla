        <PAGEOBJECT XPOS="539.370708661417" YPOS="78377.1475254358" OwnPage="258" ItemID="257179820" PTYPE="4" WIDTH="255.11811023622" HEIGHT="475.254049367302" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="3.881" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L255.118 0 L255.118 475.254 L0 475.254 L0 0 Z" copath="M0 0 L255.118 0 L255.118 475.254 L0 475.254 L0 0 Z" gXpos="539.370708661417" gYpos="78377.1475254358" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="257270716" BACKITEM="257189276"/>

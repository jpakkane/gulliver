        <PAGEOBJECT XPOS="142.416321703134" YPOS="3299.83432437593" OwnPage="9" ItemID="340305356" PTYPE="4" WIDTH="255.222103493716" HEIGHT="284.423418095801" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L255.222 0 L255.222 284.423 L0 284.423 L0 0 Z" copath="M0 0 L255.222 0 L255.222 284.423 L0 284.423 L0 0 Z" gXpos="142.416321703134" gYpos="3299.83432437593" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>

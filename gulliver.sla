        <PAGEOBJECT XPOS="539.370708661417" YPOS="70487.2419074582" OwnPage="232" ItemID="235997308" PTYPE="4" WIDTH="255.11811023622" HEIGHT="475.085021675522" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="3.881" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L255.118 0 L255.118 475.085 L0 475.085 L0 0 Z" copath="M0 0 L255.118 0 L255.118 475.085 L0 475.085 L0 0 Z" gXpos="539.370708661417" gYpos="70487.2419074582" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="236020572" BACKITEM="220280268"/>

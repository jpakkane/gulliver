        <PAGEOBJECT XPOS="142.52031496063" YPOS="6758.58047244095" OwnPage="21" ItemID="2137215467" PTYPE="4" WIDTH="255.11811023622" HEIGHT="476.220472440947" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="3.881" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L255.118 0 L255.118 476.22 L0 476.22 L0 0 Z" copath="M0 0 L255.118 0 L255.118 476.22 L0 476.22 L0 0 Z" gXpos="142.52031496063" gYpos="6758.58047244095" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="2137308411" BACKITEM="2308060"/>
